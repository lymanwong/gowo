  <div class="panel panel-default">
    <div class="panel-heading">
      <h2>Comment on this workout</h2>
    </div>
      <ul>
        <li>Name: <%= @workout.name %></li>
        <li>Body part: <%= @workout.body_part %></li>
        <li>Weight: <%= @workout.weight %></li>
        <li>Sets: <%= @workout.set %></li>
        <li>Reps: <%= @workout.rep %></li>
        <li>Date: <%= @workout.date %></li>
      </ul>
      <a href="/workouts/<%= @workout.id %>/edit">Edit</a>
  </div>
